#Spring Datasource
spring.datasource.url=jdbc:mysql://localhost:3306/<DBNAME>/useSSL=false
spring.datasource.data-username=root
spring.datasource.password=savi123

#Hibernating properties
spring.jpa.properties.hibernate.dialect = org.hibernate.dialect.MySQLSOialect

#Hibernate ddl auto(create, create-drop, valide, update)
spring.jpa.hibernate.ddl-auto=update
